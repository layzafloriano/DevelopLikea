<div class="home-content container">
  <nav class="sidebar-home">
    {{> sidebar}}
  </nav>
    <div class="home-posts col-7">
      {{#each posts}}
      {{> postsFeed}}
      {{/each}}
    </div>
    <aside class="home-side col-3">
      <div class="home-openings">
        <div class="card">
          <div class="card-body">
            <h4>Opportunities: </h4>
            {{#each openings}}
            {{> openingsHome}}
            {{/each}}

            <hr>
            
            <h4>Events: </h4>
            {{#each events}}
            {{> eventsHome}}
            {{/each}}

          </div>
        </div>
      </div>
    </aside>
</div>